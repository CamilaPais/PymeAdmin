<%= simple_form_for(@supplier, html: { class: 'form-vertical' }) do |f| %>
  <div class="row">
    <div class="col-4">
      <%= f.error_notification %>
      <%= f.input :name  %>
      <%= f.input :phone_number %>
      <%= f.input :account_balance %>
      <%= f.button :submit, class: "btn-primary" %>
    </div>
  </div>
<% end %>
